{
	"name": "sveltekit-electron",
	"version": "0.0.1",
	"private": true,
	"description": "Minimal Sveltekit + Electron starter template.",
	"main": "src/electron.js",
	"scripts": {
		"dev": "NODE_ENV=dev npm run dev:all",
		"dev:all": "concurrently -n=svelte,electron -c=green,blue \"npm run dev:svelte\" \"npm run dev:electron\"",
		"dev:svelte": "svelte-kit dev -p 3333",
		"dev:electron": "electron src/electron.js",
		"precompile": "cross-env MODE=production npm run build",
		"build": "npm run build:svelte && npm run build:electron",
		"build:svelte": "svelte-kit build",
		"build:electron": "electron-builder -mwl",
		"package": "electron-forge package",
		"make": "electron-forge make",
		"publish": "electron-forge publish",
		"start": "svelte-kit start"
	},
	"browserslist": [
		"Chrome 89"
	],
	"build": {
		"appId": "com.example.app"
	},
	"devDependencies": {
		"@rollup/plugin-alias": "^3.1.2",
		"@rollup/plugin-commonjs": "^18.0.0-1",
		"@rollup/plugin-json": "^4.1.0",
		"@rollup/plugin-node-resolve": "^11.2.0",
		"@sveltejs/adapter-node": "^1.0.0-next.10",
		"@sveltejs/kit": "^1.0.0-next.58",
		"@sveltejs/vite-plugin-svelte": "^1.0.0-next.5",
		"@types/concurrently": "^6.0.1",
		"@types/electron-devtools-installer": "^2.2.0",
		"@types/electron-window-state": "^2.0.34",
		"@types/eslint": "^7.2.7",
		"@types/prettier": "^2.2.3",
		"@types/wait-on": "^5.2.0",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"chokidar": "^3.5.1",
		"concurrently": "^6.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^8.2.0",
		"electron": "^12.0.2",
		"electron-builder": "^22.10.5",
		"electron-connect": "^0.6.3",
		"electron-context-menu": "^2.5.0",
		"electron-devtools-installer": "^3.1.1",
		"electron-packager": "^15.2.0",
		"electron-reload": "^1.5.0",
		"electron-reloader": "^1.2.0",
		"electron-updater": "^4.3.8",
		"electron-window-state": "^5.0.3",
		"eslint": "^7.22.0",
		"eslint-plugin-svelte": "^1.1.2",
		"npm-run-all": "^4.1.5",
		"ora": "^5.4.0",
		"prettier": "^2.2.1",
		"prettier-plugin-svelte": "^2.2.0",
		"rollup": "^1.20.0",
		"rollup-plugin-esbuild": "^3.0.2",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-svelte": "^5.2.3",
		"rollup-plugin-terser": "^5.3.1",
		"spectron": "^14.0.0",
		"svelte": "^3.35.0",
		"svelte-check": "^1.2.6",
		"svelte-preprocess": "^4.6.9",
		"typescript": "^4.2.3",
		"vite": "^2.1.2",
		"wait-on": "^5.3.0"
	},
	"engines": {
		"node": ">=v14.16.0",
		"npm": ">=7.6.3"
	}
}
